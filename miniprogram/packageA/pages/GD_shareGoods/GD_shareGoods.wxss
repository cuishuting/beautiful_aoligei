.ripple {
  background-color: rgba(255, 0, 130, 0.5);
  border-radius: 100%;
  height: 10px;
  width: 10px;
  margin-top: -90px;
  position: absolute;
  -webkit-transform: scale(0);
}

@-webkit-keyframes ripple {
  100% {
    -webkit-transform: scale(12);
    transform: scale(12);
    background-color: transparent;
  }
}

.pic {
  width: 130rpx;
  height: 130rpx;
}

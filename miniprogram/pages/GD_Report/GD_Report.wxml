<!--miniprogram/pages/GD_Report/GD_Report.wxml-->
<view class="grid col-2 text-left bg-white solid">
	<view class="padding">肤质类型</view>
	<view class="padding">{{sskin_type}}</view>
</view>
<view class="grid col-2 text-left bg-white solid">
	<view class="padding">是否长痘</view>
	<view class="padding">{{acne_value==1?'是':'否'}}</view>
</view>
<view class="flex flex-wrap bg-white solid grid">
	<view class="flex-sub padding-sm margin-xs radius">痘痘严重程度</view>
	<view class="flex-twice padding-sm margin-xs radius">
		<view class="cu-progress round">
			<view class="bg-black" style="width:{{acne_level}}%;"></view>
		</view>
	</view>
</view>
<view class="grid col-2 text-left bg-white solid">
	<view class="padding">有无鱼尾纹</view>
	<view class="padding">{{crows_feet==1?'有':'无'}}</view>
</view>
<view class="grid col-2 text-left bg-white solid">
	<view class="padding">有无黑眼圈</view>
	<view class="padding">{{dark_circle==1?'有':'无'}}</view>
</view>
<view class="grid col-2 text-left bg-white solid">
	<view class="padding">有无眼部细纹</view>
	<view class="padding">{{eye_finelines==1?'有':'无'}}</view>
</view>
<view class="grid col-2 text-left bg-white solid">
	<view class="padding">有无眼袋</view>
	<view class="padding">{{eye_pouch==1?'有':'无'}}</view>
</view>
<view class="grid col-2 text-left bg-white solid">
	<view class="padding">前额毛孔情况</view>
	<view class="padding">{{pores_forehead==1?'前额有毛孔粗大':'前额无毛孔粗大'}}</view>
</view>
<view class="grid col-2 text-left bg-white solid">
	<view class="padding">左脸颊毛孔情况</view>
	<view class="padding">{{pores_left_cheek==1?'左脸颊有毛孔粗大':'左脸颊无毛孔粗大'}}</view>
</view>
<view class="grid col-2 text-left bg-white solid">
	<view class="padding">右脸颊毛孔情况</view>
	<view class="padding">{{pores_right_cheek==1?'右脸颊有毛孔粗大':'右脸颊无毛孔粗大'}}</view>
</view>
<view class="grid col-2 text-left bg-white solid">
	<view class="padding">有无斑点</view>
	<view class="padding">{{skin_spot==1?'有':'无'}}</view>
</view>
<view class="padding">
  <button class="cu-btn block bg-black margin-tb-sm lg" bindtap="jumptorecommend">为我推荐护肤品</button>
</view>